<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <listener>
    <listener-class>
        com.avdo.StartUp
    </listener-class>
  </listener>
  <servlet>
  	<servlet-name>PostData</servlet-name>
  	<display-name>PostData</display-name>
  	<description></description>
  	<servlet-class>com.avdo.PostData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearchFormular</servlet-name>
  	<display-name>SearchFormular</display-name>
  	<description></description>
  	<servlet-class>com.avdo.SearchFormular</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetForms</servlet-name>
  	<display-name>GetForms</display-name>
  	<description></description>
  	<servlet-class>com.avdo.GetForms</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PostFormData</servlet-name>
  	<display-name>PostFormData</display-name>
  	<description></description>
  	<servlet-class>com.avdo.PostFormData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoadFormData</servlet-name>
  	<display-name>LoadFormData</display-name>
  	<description></description>
  	<servlet-class>com.avdo.LoadFormData</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PostData</servlet-name>
  	<url-pattern>/PostData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchFormular</servlet-name>
  	<url-pattern>/SearchFormular</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetForms</servlet-name>
  	<url-pattern>/GetForms</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PostFormData</servlet-name>
  	<url-pattern>/PostFormData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoadFormData</servlet-name>
  	<url-pattern>/LoadFormData</url-pattern>
  </servlet-mapping>
</web-app>
